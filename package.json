{
  "name": "json-component-react",
  "version": "0.0.17",
  "description": "Generate components and forms in react by using json",
  "main": "dist/index.js",
  "scripts": {
    "start": "npm-run-all --parallel develop *:watch",
    "develop": "webpack serve --config settings/webpack.config.dev.js",
    "prebuild": "npm-run-all --parallel lint",
    "build": "rimraf dist && webpack --config settings/webpack.config.prod.js",
    "test": "jest",
    "test:watch": "jest --watch",
    "lint": "esw src",
    "lint:watch": "npm run lint -- --watch"
  },
  "peerDependencies": {
    "react": ">=16.0.0",
    "react-dom": ">=16.0.0"
  },
  "dependencies": {
    "bootstrap": "^4.6.0",
    "prop-types": "^15.7.2",
    "reactstrap": "^8.9.0",
    "uuid": "^8.3.2"
  },
  "devDependencies": {
    "@babel/cli": "^7.12.10",
    "@babel/core": "^7.12.10",
    "@babel/preset-env": "^7.12.11",
    "@babel/preset-react": "^7.12.10",
    "@testing-library/jest-dom": "^5.11.9",
    "babel-eslint": "^10.0.3",
    "babel-loader": "^8.2.2",
    "css-loader": "^5.0.1",
    "eslint": "^7.18.0",
    "eslint-plugin-react": "^7.22.0",
    "eslint-watch": "^7.0.0",
    "html-webpack-plugin": "^4.5.1",
    "identity-obj-proxy": "^3.0.0",
    "ignore-styles": "^5.0.1",
    "jest": "^26.6.3",
    "json-loader": "^0.5.7",
    "mini-css-extract-plugin": "^1.3.5",
    "node-sass": "^5.0.0",
    "npm-run-all": "^4.1.5",
    "optimize-css-assets-webpack-plugin": "^5.0.4",
    "react": "^17.0.1",
    "react-dom": "^17.0.1",
    "react-test-renderer": "^17.0.1",
    "sass-loader": "^10.1.1",
    "style-loader": "^2.0.0",
    "terser-webpack-plugin": "^5.1.1",
    "url-loader": "^4.1.1",
    "webpack": "^5.18.0",
    "webpack-cli": "^4.4.0",
    "webpack-dev-middleware": "^4.1.0",
    "webpack-dev-server": "^3.11.2",
    "webpack-hot-middleware": "^2.25.0"
  },
  "babel": {
    "presets": [
      "@babel/preset-env",
      "@babel/preset-react"
    ]
  },
  "jest": {
    "moduleNameMapper": {
      "^.+\\.(css|less|scss)$": "identity-obj-proxy"
    }
  },
  "author": "Idan Avrahami",
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "https://github.com/idanavr/json-component-react"
  },
  "keywords": [
    "react",
    "json",
    "component",
    "generator",
    "form",
    "javascript",
    "ui",
    "library"
  ]
}