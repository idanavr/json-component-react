{
  "name": "json-component-react",
  "version": "0.0.16",
  "description": "Generate components and forms in react by using json",
  "main": "dist/index.js",
  "scripts": {
    "start": "npm-run-all --parallel development *:watch",
    "development": "webpack-dev-server --open --config settings/webpack.config.dev.js",
    "prebuild": "npm-run-all --parallel lint",
    "build": "rimraf dist && webpack --config settings/webpack.config.prod",
    "lint": "esw src",
    "lint:watch": "npm run lint -- --watch"
  },
  "peerDependencies": {
    "react": ">=16.0.0",
    "react-dom": ">=16.0.0"
  },
  "dependencies": {
    "bootstrap": "^4.4.1",
    "prop-types": "^15.7.2",
    "react-tooltip": "^4.2.6",
    "reactstrap": "^8.4.1",
    "uuid": "^8.0.0"
  },
  "devDependencies": {
    "@babel/cli": "^7.8.4",
    "@babel/core": "^7.9.6",
    "@babel/preset-env": "^7.9.6",
    "@babel/preset-react": "^7.8.3",
    "babel-eslint": "^10.0.3",
    "babel-loader": "^8.0.6",
    "css-loader": "^3.5.3",
    "eslint": "^7.0.0",
    "eslint-plugin-react": "^7.18.0",
    "eslint-watch": "^6.0.1",
    "html-webpack-plugin": "^4.3.0",
    "ignore-styles": "^5.0.1",
    "json-loader": "^0.5.7",
    "mini-css-extract-plugin": "^0.9.0",
    "node-sass": "^4.14.1",
    "npm-run-all": "^4.1.5",
    "optimize-css-assets-webpack-plugin": "^5.0.3",
    "react": "^16.13.1",
    "react-dom": "^16.13.1",
    "sass-loader": "^8.0.2",
    "style-loader": "^1.2.1",
    "terser-webpack-plugin": "^3.0.1",
    "url-loader": "^4.1.0",
    "webpack": "^4.42.0",
    "webpack-cli": "^3.3.11",
    "webpack-dev-middleware": "^3.7.2",
    "webpack-dev-server": "^3.11.0",
    "webpack-hot-middleware": "^2.25.0"
  },
  "babel": {
    "presets": [
      "@babel/preset-env",
      "@babel/preset-react"
    ]
  },
  "author": "Idan Avrahami",
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "https://github.com/idanavr/json-component-react"
